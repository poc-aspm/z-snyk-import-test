  "scripts": {
    "setup": "bfa-next",
    "dev": "bfa-next && BUSINESS_ID=ipsymx node server.js",
    "build": "BUILD_PHASE=true BUSINESS_ID=ipsymx next build && cp emptyresponse.html .next/static/",
    "start": "cross-env NODE_ENV=production BUSINESS_ID=ipsymx node server.js",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit", 
    "lint": "eslint . --fix --ext .ts,.tsx",
    "lint:ci": "eslint . --ext .ts,.tsx",
    "test": "bfa-next && cross-env NODE_ENV=test jest --watch --passWithNoTests",
    "test:showlogs": "cross-env NODE_ENV=test jest --silent false --verbose false --passWithNoTests",
    "test:ci": "cross-env NODE_ENV=test jest --ci --coverage --no-cache --passWithNoTests",
    "generate-types:local": "NODE_TLS_REJECT_UNAUTHORIZED=0 npm run generate-types",
    "generate-types": "graphql-codegen && prettier --write ./gql.generated.ts",
    "sonar": "node sonar-project.js"
